{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "BROWSER=none react-scripts start",
    "test": "react-scripts test --verbose --coverage --watchAll false",
    "build": "react-scripts build",
    "start": "serve -s build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "serve": "^14.2.1",
    "socket.io-client": "^4.7.2",
    "web-vitals": "^3.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.2",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "!<rootDir>/src/*.js",
      "!<rootDir>/src/index.jsx",
      "!<rootDir>/src/hooks/*.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.x.x <=20.x.x"
  },
  "license": "MIT"
}
